{
  "name": "platform-web-ui-e2e",
  "version": "1.2.3",
  "description": "Platform UI E2E Test Scenarios",
  "scripts": {
    "build": "tsc",
    "postinstall": "npx playwright install",
    "pretest": "rimraf ./common/*.xml && rimraf ./common/results && rimraf ./build",
    "test": "playwright test",
    "test:smoke": "npm test -- --grep='@smoke' --workers=4",
    "test:criticalPath": "npm test -- --grep='@criticalPath'",
    "report": "npx playwright show-report ./common/html-report",
    "lint": "eslint . --fix",
    "prepare": "husky install"
  },
  "prettier": {
    "printWidth": 120
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/_bcgroup/platform/core_svcs/platform-web-ui-e2e.git"
  },
  "keywords": [],
  "author": "Dzmitry_Shaplyka@epam.com",
  "license": "MIT",
  "dependencies": {
    "@playwright/test": "1.19.2",
    "csvtojson": "^2.0.10",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.34",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.2",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint": "^8.10.0",
    "eslint-config-google": "^0.14.0"
  }
}